{
    "name": "DAFC Corporate Theme",
    "dataColors": [
        "#1E3A5F",
        "#D4AF37",
        "#0D9488",
        "#3B82F6",
        "#22C55E",
        "#F59E0B",
        "#EF4444",
        "#8B5CF6",
        "#EC4899",
        "#6366F1"
    ],
    "background": "#FFFFFF",
    "foreground": "#1E3A5F",
    "tableAccent": "#D4AF37",
    "visualStyles": {
        "*": {
            "*": {
                "*": [
                    {
                        "fontFamily": "Inter, Segoe UI, sans-serif"
                    }
                ],
                "title": [
                    {
                        "fontFamily": "Plus Jakarta Sans, Segoe UI, sans-serif",
                        "fontSize": 14,
                        "fontColor": {"solid": {"color": "#1E3A5F"}},
                        "fontWeight": "600"
                    }
                ],
                "subTitle": [
                    {
                        "fontFamily": "Inter, Segoe UI, sans-serif",
                        "fontSize": 11,
                        "fontColor": {"solid": {"color": "#64748B"}}
                    }
                ],
                "labels": [
                    {
                        "fontFamily": "Inter, Segoe UI, sans-serif",
                        "fontSize": 10,
                        "fontColor": {"solid": {"color": "#475569"}}
                    }
                ],
                "categoryAxis": [
                    {
                        "labelColor": {"solid": {"color": "#475569"}},
                        "fontSize": 10,
                        "fontFamily": "Inter, Segoe UI, sans-serif",
                        "gridlineColor": {"solid": {"color": "#E2E8F0"}},
                        "gridlineStyle": "solid",
                        "gridlineShow": true
                    }
                ],
                "valueAxis": [
                    {
                        "labelColor": {"solid": {"color": "#475569"}},
                        "fontSize": 10,
                        "fontFamily": "Inter, Segoe UI, sans-serif",
                        "gridlineColor": {"solid": {"color": "#E2E8F0"}},
                        "gridlineStyle": "dashed",
                        "gridlineShow": true
                    }
                ],
                "legend": [
                    {
                        "fontFamily": "Inter, Segoe UI, sans-serif",
                        "fontSize": 10,
                        "labelColor": {"solid": {"color": "#475569"}}
                    }
                ]
            }
        },
        "card": {
            "*": {
                "calloutValue": [
                    {
                        "fontFamily": "Plus Jakarta Sans, Segoe UI, sans-serif",
                        "fontSize": 28,
                        "fontWeight": "700",
                        "color": {"solid": {"color": "#1E3A5F"}}
                    }
                ],
                "category": [
                    {
                        "fontFamily": "Inter, Segoe UI, sans-serif",
                        "fontSize": 11,
                        "color": {"solid": {"color": "#64748B"}}
                    }
                ]
            }
        },
        "slicer": {
            "*": {
                "header": [
                    {
                        "fontFamily": "Plus Jakarta Sans, Segoe UI, sans-serif",
                        "fontColor": {"solid": {"color": "#1E3A5F"}},
                        "fontSize": 12,
                        "fontWeight": "600",
                        "background": {"solid": {"color": "#F8FAFC"}}
                    }
                ],
                "items": [
                    {
                        "fontFamily": "Inter, Segoe UI, sans-serif",
                        "fontColor": {"solid": {"color": "#334155"}},
                        "fontSize": 11,
                        "background": {"solid": {"color": "#FFFFFF"}}
                    }
                ]
            }
        },
        "table": {
            "*": {
                "columnHeaders": [
                    {
                        "fontFamily": "Plus Jakarta Sans, Segoe UI, sans-serif",
                        "fontSize": 11,
                        "fontWeight": "600",
                        "fontColor": {"solid": {"color": "#FFFFFF"}},
                        "backColor": {"solid": {"color": "#1E3A5F"}}
                    }
                ],
                "values": [
                    {
                        "fontFamily": "Inter, Segoe UI, sans-serif",
                        "fontSize": 11,
                        "fontColor": {"solid": {"color": "#334155"}},
                        "backColor": {"solid": {"color": "#FFFFFF"}}
                    }
                ],
                "total": [
                    {
                        "fontFamily": "Plus Jakarta Sans, Segoe UI, sans-serif",
                        "fontSize": 11,
                        "fontWeight": "600",
                        "fontColor": {"solid": {"color": "#1E3A5F"}},
                        "backColor": {"solid": {"color": "#FEF3C7"}}
                    }
                ],
                "grid": [
                    {
                        "outlineColor": {"solid": {"color": "#E2E8F0"}},
                        "outlineWeight": 1,
                        "gridVertical": true,
                        "gridHorizontal": true,
                        "gridVerticalColor": {"solid": {"color": "#F1F5F9"}},
                        "gridHorizontalColor": {"solid": {"color": "#E2E8F0"}}
                    }
                ]
            }
        },
        "pivotTable": {
            "*": {
                "columnHeaders": [
                    {
                        "fontFamily": "Plus Jakarta Sans, Segoe UI, sans-serif",
                        "fontSize": 11,
                        "fontWeight": "600",
                        "fontColor": {"solid": {"color": "#FFFFFF"}},
                        "backColor": {"solid": {"color": "#1E3A5F"}}
                    }
                ],
                "rowHeaders": [
                    {
                        "fontFamily": "Plus Jakarta Sans, Segoe UI, sans-serif",
                        "fontSize": 11,
                        "fontWeight": "500",
                        "fontColor": {"solid": {"color": "#1E3A5F"}},
                        "backColor": {"solid": {"color": "#F8FAFC"}}
                    }
                ],
                "values": [
                    {
                        "fontFamily": "Inter, Segoe UI, sans-serif",
                        "fontSize": 11,
                        "fontColor": {"solid": {"color": "#334155"}}
                    }
                ]
            }
        }
    },
    "textClasses": {
        "title": {
            "fontFace": "Plus Jakarta Sans, Segoe UI, sans-serif",
            "fontSize": 16,
            "fontWeight": "700",
            "color": "#1E3A5F"
        },
        "header": {
            "fontFace": "Plus Jakarta Sans, Segoe UI, sans-serif",
            "fontSize": 14,
            "fontWeight": "600",
            "color": "#1E3A5F"
        },
        "label": {
            "fontFace": "Inter, Segoe UI, sans-serif",
            "fontSize": 11,
            "fontWeight": "400",
            "color": "#475569"
        },
        "callout": {
            "fontFace": "Plus Jakarta Sans, Segoe UI, sans-serif",
            "fontSize": 28,
            "fontWeight": "700",
            "color": "#1E3A5F"
        },
        "largeTitleText": {
            "fontFace": "Plus Jakarta Sans, Segoe UI, sans-serif",
            "fontSize": 24,
            "fontWeight": "700",
            "color": "#1E3A5F"
        },
        "smallLightText": {
            "fontFace": "Inter, Segoe UI, sans-serif",
            "fontSize": 10,
            "fontWeight": "400",
            "color": "#94A3B8"
        }
    },
    "page": {
        "backgroundColor": "#F8FAFC",
        "wallpaperBrush": {
            "type": "solid",
            "color": "#F8FAFC"
        }
    },
    "sentimentColors": {
        "negative": "#EF4444",
        "neutral": "#94A3B8",
        "positive": "#22C55E"
    },
    "kpiColors": {
        "good": "#22C55E",
        "neutral": "#F59E0B",
        "bad": "#EF4444"
    }
}
